{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "env": {
      "VITE_API_BASE_URL": "https://personal-notes-backend.vercel.app/api"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  